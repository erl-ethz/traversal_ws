<?xml version="1.0"?>
<launch>

  <arg name="quad_name" default="khadas_drone"/>

  <group ns="$(arg quad_name)" >

    <!-- <node pkg="joy" type="joy_node" name="joy_node">
      <param name="autorepeat_rate" value="10"/>
    </node> -->
    
    <node pkg="trajectory_generator"
        type="traj_gen"
        name="trajectory_generator"
        output="screen" >
      <rosparam file="$(find trajectory_generator)/parameters/traj.yaml" />
    </node>

    <node name="rqt_quad_gui" pkg="rqt_gui" type="rqt_gui" 
        args="-s rqt_quad_gui.basic_flight.BasicFlight --args 
        --quad_name $(arg quad_name)" output="screen"/>

  </group>

</launch>
